diff --git a/A3_IOS/CompSubviewsVC.swift b/A3_IOS/CompSubviewsVC.swift
new file mode 100644
index 0000000..830769b
--- /dev/null
+++ b/A3_IOS/CompSubviewsVC.swift
@@ -0,0 +1,41 @@
+//
+//  CompSubviewsVC.swift
+//  A3_IOS
+//
+//  Created by Rhea Shah on 3/11/25.
+//
+
+import UIKit
+
+class CompSubviewsVC: UIViewController {
+    @IBOutlet weak var lineupView: UIView!
+    @IBOutlet weak var judgingView: UIView!
+    @IBOutlet weak var mediaView: UIView!
+
+    override func viewDidLoad() {
+        super.viewDidLoad()
+        
+        // Hide all views initially
+        hideAllSubviews()
+    }
+    
+    // Function to hide all subviews
+    func hideAllSubviews() {
+        lineupView.isHidden = true
+        judgingView.isHidden = true
+        mediaView.isHidden = true
+    }
+
+    // Functions to show individual subviews
+    func showLineupView() {
+        lineupView.isHidden = false
+    }
+    
+    func showJudgingView() {
+        judgingView.isHidden = false
+    }
+    
+    func showMediaView() {
+        mediaView.isHidden = false
+    }
+}
